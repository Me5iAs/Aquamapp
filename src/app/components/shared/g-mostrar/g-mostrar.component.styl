h1
  color:rgba(0, 0, 0, 0.87);
  display:block;
  font-family:Roboto, "Helvetica Neue", sans-serif;
  font-size:20px;
  font-weight:500;
  height:24px;
  margin-bottom:12px;

h2
  font-family: Roboto,"Helvetica Neue",sans-serif;
  font-size: 14px;
  color:rgba(0, 0, 0, 0.54);
  display:block;
  height:17px;
  margin-bottom:16px;
  margin-top:-8px;

table
  width: 100%

  th, td
    padding-right: 5px
  
  th:first-child
    padding-left: 10px;
  th:last-child
    padding-right: 5px;
  

  tr
    td:first-child
      padding-left: 10px
    td:last-child
      padding-right: 5px

.container
  max-width 80%
  margin: 0px auto;

@media screen and (max-width:667px)
  mat-dialog-container
    padding: 0px
  .container
    max-width: 100%

.acciones
  padding 0px !important
  margin 0px !important

  mat-icon
    cursor pointer